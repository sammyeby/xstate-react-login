{
  "name": "xstate-react-login",
  "version": "0.1.0",
  "description": "A demo application with Login and Signup features for quick project bootstrap",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/xstate-react-login.umd.cjs",
  "module": "./dist/xstate-react-login.js",
  "types": "./dist/xstate-react-login.d.ts",
  "exports": {
    ".": {
      "import": "./dist/xstate-react-login.js",
      "require": "./dist/xstate-react-login.umd.cjs"
    }
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sammyeby/xstate-react-login.git"
  },
  "keywords": [
    "react",
    "typescript",
    "xstate",
    "xstate/react",
    "demo-login"
  ],
  "author": "Samuel Umeh <contact@samuelumeh.com> (https://samuelumeh.com)",
  "peerDependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/material": "^5.14.19",
    "@xstate/react": "^3.2.2",
    "axios": "^1.6.2",
    "final-form": "^4.20.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-final-form": "^6.5.9",
    "react-router-dom": "^6.20.1",
    "xstate": "^4.38.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "sass": "^1.69.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.7",
    "vite-plugin-dts": "^3.6.4",
    "vite-tsconfig-paths": "^4.2.1"
  }
}
